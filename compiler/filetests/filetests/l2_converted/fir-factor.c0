int run() {
  // return the number of unique prime factors the input has.

  int in = 24150;
  int numfactors = 0;
  int i = 2;
  while (in != 1) {
    if (in % i == 0) {
      // i is a factor of in. we can't save or print it yet, but...
      in /= i;
      numfactors += 1;
    } else {
      i += 1;
    }
  }

  return numfactors;
}


void main() { assert (run() == (6)); }
