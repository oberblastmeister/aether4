int run() {
  /* Tests for loops, with break and continue */
  int n;
  bool break = false;
  for (n = -1234; !break && 42 != 0; n += 1) {
    if (n != 12) {
    } else {
        break = true;
    }
  }

  int c;
  for (c = n; c != 0; c -= 1) {
    n += 1;
  }

  return n;
}


void main() { assert (run() == (26)); }
