// Calculate the nth prime number
int run() {
  int n = 40;
  int i;
  for (i = 2; n > 0; i += 1)
  {
    int x = 1;
    int j;
    bool break = false;
    for (j = 2; !break && j < i - 1; j += 1)
      if (i % j == 0)
      {
        x = 0;
        break = true;
      }
    if (x != 0)
      n -= 1;
  }
  return i - 1;
}


void main() { assert (run() == (173)); }
