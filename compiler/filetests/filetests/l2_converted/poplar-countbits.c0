// count the number of one bits in 0xABCDEF01
int run() {
        int num = 2882400001;
        int count = 0;
        int i;
        for (i = 1; i != 0; i <<= 1)
                if ((i & num) != 0)
                        count += 1;
        return count;
}


void main() { assert (run() == (18)); }
