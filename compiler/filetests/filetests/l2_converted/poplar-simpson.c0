// compute integral x^3 from 0 to 1 by Simpson's rule
int run() {
        int a = 0;
        int b = 100;
        int sum = 0;
        int i;
        for (i = a; i < b; i += (b - a)/4) {
                int mid = i + (b - a)/8;
                int i1 = i + (b - a)/4;
                sum += (b - a)/24 * ((i*i*i) + 4 * (mid*mid*mid)
                        + (i1*i1*i1));
        }
        return sum;
}


void main() { assert (run() == (23687392)); }
