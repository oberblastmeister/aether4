// count the primes less than 100
int main() {
        int count = 0;
        int i;
        for (i = 2; i < 100; i += 1) {
                int j;
                for (j = 2; j < i; j += 1) {
                        if (i % j == 0)
                                break;
                }
                if (j == i)
                        count += 1;
        }
        return count;
}
