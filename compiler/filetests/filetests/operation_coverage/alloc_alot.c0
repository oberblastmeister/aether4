int sum(int[] a, int n) {
    int res = 0;
    for (int i = 0; i < n; i++) {
        res += a[i];
    }
    return res;
}

void main() {
    int n = 1000000;
    int[] a = alloc_array(int, n);
    for (int i = 0; i < n; i++) {
        a[i] = i;
    }
    int res = sum(a, n);
}
