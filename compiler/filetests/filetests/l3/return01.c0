int sum(int i, int j) {
  int sum;
  sum = 0;
  for (; i <= j; i+=1)
    sum += i;
  return sum;
}

/* Simple function call to sum up numbers */
int run() {
  int n;
  int x;
  int y;
  n = 100;
  x = sum(1,100);
  y = n*(n+1)/2;
  return x-y;
}


void main() { assert (run() == (0)); }
