int main() {
  int n = 5;

  int num = 2*n;

  int denom = n + 1;

  //compute n!
  int x=1;
  while(n != 0)
  {
    x*=n; 
    n-=1;
  }
  n = x;

  //compute num!
  x=1;
  while(num != 0)
  {
    x*=num; 
    num-=1;
  }
  num = x;

  //compute denom!
  x=1;
  while(denom != 0)
  {
    x*=denom; 
    denom-=1;
  }
  denom = x;
  
  return num/(denom*n);
}
