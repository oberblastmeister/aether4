int run() {
  int N = 200;
  
  if (N <= 0) return 0;
  else if (N == 1) return 2;
  else {
    int prime = 1;
    for (int found = 1; found < N; found += 1) {
      prime += 2;
      bool break = false;
      for (int i = 3; !break && prime >= i * i; i += 2) {
        if (prime % i != 0) {
        } else {
            found -= 1;
            break = true;
        }
      }
    }
    return prime;
  }
}


void main() { assert (run() == (1223)); }
